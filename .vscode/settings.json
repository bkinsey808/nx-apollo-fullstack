{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.sideBar.location": "right",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "github.copilot.autocomplete.enable": true,
  "cSpell.words": [
    "GraphQL",
    "buildable",
    "builtins",
    "cacheable",
    "chainable",
    "codegen",
    "cssmodule",
    "eqeqeq",
    "mixins",
    "nrwl",
    "openid",
    "pkce",
    "queryable",
    "rerender",
    "sonarjs",
    "unmount"
  ],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".vscode-insiders",
    "package.json",
    "*.initialState.json",
    "*.testData.json",
    "**/*.d.ts",
    ".vscode-server"
  ]
}
