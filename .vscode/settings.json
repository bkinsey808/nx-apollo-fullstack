{
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "editor.formatOnSave": true,
  "workbench.sideBar.location": "right",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cSpell.words": [
    "codegen",
    "nrwl",
    "queryable",
    "cssmodule"
  ],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".vscode-insiders",
    "package.json",
    "*.initialState.json",
    "*.testData.json",
    "**/*.d.ts",
    ".vscode-server"
  ],
}
