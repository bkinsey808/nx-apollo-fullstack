{
  "name": "nx-graphql-fullstack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "client:build:prod": "nx build client --prod -- --memoryLimit=8192",
    "client:build:prod:stats": "nx build client --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "client:build:dev:stats": "nx build client --skip-nx-cache -- --memoryLimit=8192  --statsJson",
    "analyze:prod:stats": "run-s \"client:build:prod:stats\" \"webpack-bundle-analyzer\"",
    "analyze:dev:stats": "run-s \"client:build:dev:stats\" \"webpack-bundle-analyzer\"",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/client/stats.json",
    "test": "nx run-many --all --target=test",
    "client:test-watch": "nx test client --watch",
    "lint": "nx run-many --all --target=lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "codegen:schema-from-build": "node dist/apps/api/main.js -g",
    "codegen:schema-from-endpoint": "run-s \"codegen:schema-from-endpoint-json\" \"codegen:schema-from-endpoint-gql\"",
    "codegen:schema-from-endpoint-json": "npx apollo client:download-schema --endpoint=http://localhost:4000/graphql libs/api-interfaces/schema/schema.json",
    "codegen:schema-from-endpoint-gql": "npx apollo client:download-schema --endpoint=http://localhost:4000/graphql libs/api-interfaces/schema/schema.gql",
    "codegen:generate": "apollo client:codegen --localSchemaFile=libs/api-interfaces/schema/schema.json --includes=apps/client/src/**/*.tsx --target=typescript --globalTypesFile=libs/api-interfaces/__generated__/globalTypes.ts",
    "watch": "run-p \"watch-api\" \"watch-schema\" \"watch-client\"",
    "watch-api": "nodemon -w apps/api/src -x \"npm run codegen:schema-from-endpoint\" -e *.ts",
    "watch-schema": "nodemon -w libs/api-interfaces/schema/schema.json -x \"npm run codegen:generate\"",
    "watch-client": "nodemon -w apps/client/src -x \"npm run codegen:generate\" --ignore **/__generated__/** -e tsx,ts",
    "sleep": "sleep",
    "sleep-and-watch-client": "run-s \"sleep -- 5s\" \"watch\" ",
    "dev": "run-p \"nx serve api\" \"nx serve client\" \"sleep-and-watch-client\" ",
    "generate-urql-schema": "ts-node ./apps/api/src/scripts/urqlIntrospection.ts"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@material-ui/core": "^4.12.1",
    "@okta/okta-auth-js": "^5.3.1",
    "@okta/okta-react": "^6.1.0",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-graphcache": "^4.3.0",
    "@urql/introspection": "^0.3.0",
    "apollo-server-express": "^2.25.2",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.4",
    "document-register-element": "1.13.1",
    "express": "4.17.1",
    "graphql-modules": "^1.4.3",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.14.1",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "0.13.7",
    "ts-deepmerge": "^1.0.8",
    "tslib": "^2.0.0",
    "urql": "^2.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@nrwl/cli": "12.4.0",
    "@nrwl/cypress": "12.4.0",
    "@nrwl/eslint-plugin-nx": "12.4.0",
    "@nrwl/express": "^12.4.0",
    "@nrwl/jest": "12.4.0",
    "@nrwl/linter": "12.4.0",
    "@nrwl/node": "12.4.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "12.4.0",
    "@nrwl/tao": "^12.4.0",
    "@nrwl/web": "12.4.0",
    "@nrwl/workspace": "12.4.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.0",
    "@types/jest": "26.0.8",
    "@types/lodash": "^4.14.170",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "apollo": "^2.33.4",
    "babel-jest": "26.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "cli-sleep": "^1.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^7.3.0",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "jest": "27.0.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.2.4",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
